{
  "name": "@applitools/eyes-nightwatch",
  "version": "1.15.0",
  "description": "Applitools Eyes SDK for Nightwatch.js",
  "keywords": [
    "eyes-nightwatch",
    "eyes-rendering",
    "eyes-visual-grid",
    "applitools",
    "eyes",
    "test automation",
    "visual regression",
    "automation",
    "testing",
    "tests",
    "nightwatch",
    "nightwatchjs"
  ],
  "homepage": "https://applitools.com",
  "bugs": {
    "url": "https://github.com/applitools/eyes.sdk.javascript1/issues"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/applitools/eyes.sdk.javascript1.git",
    "directory": "js/packages/eyes-nightwatch"
  },
  "license": "SEE LICENSE IN LICENSE",
  "author": {
    "name": "Applitools Team",
    "email": "team@applitools.com"
  },
  "aliases": [
    "nightwatch"
  ],
  "main": "./dist/index.js",
  "types": "./types/index.d.ts",
  "files": [
    "dist",
    "commands",
    "types"
  ],
  "scripts": {
    "lint": "eslint '**/*.ts' '**/*.js'",
    "build": "tspc --project ./tsconfig.build.json",
    "generate:tests": "coverage-tests generate ./test/coverage/config.js --name 'eyes-nightwatch'",
    "test": "npm run test:it && npm run test:e2e && npm run test:coverage",
    "test:it": "MOCHA_GROUP=it mocha './test/it/*.spec.ts' -r @applitools/test-utils/mocha-hooks/docker",
    "test:e2e": "nightwatch --config test/e2e/nightwatch.conf.js --eyes-config applitools.config.js test/e2e/*.spec.js",
    "test:coverage": "npm run generate:tests && APPLITOOLS_BATCH_NAME='JS Coverage Tests: eyes-nightwatch' APPLITOOLS_BATCH_ID=$(uuidgen) MOCHA_GROUP=coverage mocha './test/generic/*.spec.js' -r @applitools/test-utils/mocha-hooks/docker --parallel --jobs ${MOCHA_JOBS:-15} --exit",
    "setup": "npm run docker:setup",
    "docker:setup": "node ../../scripts/generate-docker-compose-config.js && docker-compose up -d",
    "docker:teardown": "docker-compose down",
    "upgrade:framework": "if [ ! -z $APPLITOOLS_FRAMEWORK_VERSION ]; then npm i --no-save nightwatch@$APPLITOOLS_FRAMEWORK_VERSION; fi",
    "report": "bongo report --name 'nightwatch'",
    "postpublish": "bongo send-release-notification --name 'nightwatch'"
  },
  "dependencies": {
    "@applitools/driver": "1.12.0",
    "@applitools/eyes": "1.2.11",
    "@applitools/spec-driver-webdriver": "1.0.32",
    "@applitools/utils": "1.3.37",
    "webdriver": "7.27.0"
  },
  "devDependencies": {
    "@applitools/api-extractor": "1.2.21",
    "@applitools/bongo": "^3.0.4",
    "@applitools/sdk-coverage-tests": "3.0.2",
    "@applitools/test-utils": "1.5.17",
    "@types/nightwatch": "^2.3.16",
    "@types/node": "^12.20.55",
    "chromedriver": "^110.0.0",
    "cucumber": "6.0.7",
    "geckodriver": "^3.0.1",
    "nightwatch": "^2.5.4"
  },
  "peerDependencies": {
    "nightwatch": ">=1.3.4"
  },
  "engines": {
    "node": ">=12.13.0"
  }
}
