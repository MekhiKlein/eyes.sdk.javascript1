{
  "name": "@applitools/eyes-puppeteer",
  "version": "1.17.1",
  "description": "Applitools Eyes SDK for Puppeteer",
  "keywords": [
    "eyes-puppeteer",
    "applitools",
    "eyes",
    "test automation",
    "visual regression",
    "automation",
    "testing",
    "tests",
    "puppeteer"
  ],
  "homepage": "https://applitools.com",
  "bugs": {
    "url": "https://github.com/applitools/eyes.sdk.javascript1/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:applitools/eyes.sdk.javascript1.git",
    "directory": "js/packages/eyes-puppeteer"
  },
  "license": "SEE LICENSE IN LICENSE",
  "author": {
    "name": "Applitools Team",
    "email": "team@applitools.com"
  },
  "aliases": [
    "puppeteer",
    "pptr"
  ],
  "main": "./dist/index.js",
  "types": "./types/index.d.ts",
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "lint": "run --top-level eslint '**/*.ts'",
    "build": "run --top-level tspc --project ./tsconfig.build.json",
    "generate:tests": "coverage-tests generate https://raw.githubusercontent.com/applitools/sdk.coverage.tests/universal-sdk/js/config.js --name 'eyes-puppeteer'",
    "test": "yarn test:coverage",
    "test:coverage": "yarn generate:tests && APPLITOOLS_BATCH_NAME='JS Coverage Tests: eyes-puppeteer' APPLITOOLS_BATCH_ID=$(uuidgen) MOCHA_GROUP=coverage run --top-level mocha './test/generic/*.spec.js' --parallel --jobs ${MOCHA_JOBS:-15} --exit",
    "upgrade:framework": "if [ ! -z $APPLITOOLS_FRAMEWORK_VERSION ]; then yarn add --no-lockfile puppeteer@$APPLITOOLS_FRAMEWORK_VERSION; fi",
    "report": "bongo report --name 'puppeteer'",
    "postpublish": "bongo send-release-notification --name 'puppeteer'"
  },
  "dependencies": {
    "@applitools/eyes": "1.2.14",
    "@applitools/spec-driver-puppeteer": "1.1.64"
  },
  "devDependencies": {
    "@applitools/api-extractor": "1.2.21",
    "@applitools/bongo": "^4.0.0",
    "@applitools/sdk-coverage-tests": "3.0.2",
    "@applitools/test-utils": "1.5.17",
    "@types/node": "^12.20.55",
    "puppeteer": "^18.0.5"
  },
  "peerDependencies": {
    "puppeteer": ">=5.3.0"
  },
  "engines": {
    "node": ">=12.13.0"
  }
}
