{
  "name": "@applitools/universal-client",
  "version": "1.0.0",
  "description": "Applitools Eyes Universal client for Universal SDK's",
  "keywords": [
    "applitools",
    "eyes",
    "test automation",
    "visual regression",
    "automation",
    "testing",
    "tests"
  ],
  "publishConfig": {
    "access": "restricted"
  },
  "homepage": "https://applitools.com",
  "bugs": {
    "url": "https://github.com/applitools/eyes.sdk.javascript1/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:applitools/eyes.sdk.javascript1.git",
    "directory": "js/packages/eyes-universal"
  },
  "license": "SEE LICENSE IN LICENSE",
  "author": {
    "name": "Applitools Team",
    "email": "team@applitools.com"
  },
  "exports": {
    "./webdriver": "./dist/webdriver",
    "./dist/webdriver": "./dist/webdriver",
    "./custom": "./dist/custom",
    "./package.json": "./package.json",
    ".": "./dist/webdriver/index.js"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/webdriver/index.js",
  "scripts": {
    "lint": "eslint '**/*.ts'",
    "build": "tsc",
    "docker:setup": "node ../scripts/scripts/generate-docker-compose-config.js && docker-compose up -d",
    "docker:teardown": "docker-compose down",
    "test": "yarn test:unit && yarn test:it && yarn test:e2e",
    "test:sanity": "echo \"sanity\"",
    "test:unit": "mocha ./test/unit/*.spec.ts --exit --no-timeouts -r ts-node/register",
    "test:it": "mocha ./test/it/*.spec.ts --no-timeouts -r ts-node/register",
    "test:e2e": "mocha ./test/e2e/*.spec.ts --exit --no-timeouts -r ts-node/register",
    "deps": "bongo deps",
    "gh:publish": "gh workflow run publish-universal-client.yml --ref $(git rev-parse --abbrev-ref HEAD)",
    "preversion": "bongo preversion --verifyPendingChanges && yarn build",
    "version": "yarn build && bongo version --withPendingChanges",
    "postversion": "bongo postversion"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn bongo lint"
    }
  },
  "dependencies": {
    "@applitools/eyes-api": "1.7.4",
    "@applitools/logger": "1.1.15",
    "@applitools/spec-driver-selenium": "1.3.15",
    "ws": "7.4"
  },
  "devDependencies": {
    "@applitools/bongo": "^2.1.6",
    "@applitools/sdk-coverage-tests": "^2.3.19",
    "@applitools/test-server": "^1.1.4",
    "@applitools/test-utils": "^1.4.2",
    "@applitools/types": "1.5.6",
    "@types/mocha": "^9.0.0",
    "@types/node": "12",
    "@types/ws": "^7.4.4",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "chai": "^4.3.6",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-mocha-no-only": "^1.1.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "mocha": "^9.0.0",
    "prettier": "^2.6.2",
    "selenium-webdriver": "^4.3.0",
    "ts-node": "^10.0.0",
    "typescript": "^4.6.4"
  },
  "engines": {
    "node": ">=12.13.0"
  }
}
