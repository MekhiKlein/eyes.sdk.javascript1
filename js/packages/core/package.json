{
  "name": "@applitools/core",
  "version": "0.0.1",
  "homepage": "https://applitools.com",
  "bugs": {
    "url": "https://github.com/applitools/eyes.sdk.javascript1/issues"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/applitools/eyes.sdk.javascript1.git",
    "directory": "js/packages/core-base"
  },
  "license": "SEE LICENSE IN LICENSE",
  "author": {
    "name": "Applitools Team",
    "email": "team@applitools.com"
  },
  "aliases": [
    "core"
  ],
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "default": "./dist/index.js"
    },
    "./dist/*": "./dist/*.js",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "types": "./types/index.d.ts",
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "lint": "eslint '**/*.ts'",
    "build": "tsc",
    "test": "yarn test:unit",
    "test:sanity": "yarn test:unit",
    "test:unit": "mocha --no-timeouts -r ts-node/register ./test/unit/*.spec.ts",
    "deps": "bongo deps",
    "preversion": "bongo preversion",
    "version": "bongo version",
    "postversion": "bongo postversion --skip-release-notification"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn bongo lint"
    }
  },
  "dependencies": {
    "@applitools/core-base": "link:../core-base",
    "@applitools/dom-capture": "11.1.1",
    "@applitools/dom-snapshot": "4.6.2",
    "@applitools/driver": "1.9.20",
    "@applitools/logger": "1.1.14",
    "@applitools/req": "link:../req",
    "@applitools/screenshoter": "3.4.14",
    "@applitools/snippets": "^2.4.5",
    "@applitools/types": "1.5.6",
    "@applitools/utils": "1.3.10"
  },
  "devDependencies": {
    "@applitools/bongo": "^2.1.6",
    "@applitools/test-utils": "^1.4.3",
    "@types/mocha": "^9.1.1",
    "@types/node": "14",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-mocha-no-only": "^1.1.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^4.3.7",
    "mocha": "^10.0.0",
    "nock": "^13.2.8",
    "prettier": "^2.6.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  },
  "engines": {
    "node": ">=14.15.0"
  }
}
