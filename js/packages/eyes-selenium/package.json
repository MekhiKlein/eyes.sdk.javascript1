{
  "name": "@applitools/eyes-selenium",
  "version": "4.66.2",
  "description": "Applitools Eyes SDK for Selenium WebDriver",
  "keywords": [
    "eyes-selenium",
    "eyes-rendering",
    "eyes-visual-grid",
    "applitools",
    "eyes",
    "test automation",
    "visual regression",
    "automation",
    "testing",
    "tests",
    "selenium",
    "webdriver",
    "webdriverjs"
  ],
  "homepage": "https://applitools.com",
  "bugs": {
    "url": "https://github.com/applitools/eyes.sdk.javascript1/issues"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/applitools/eyes.sdk.javascript1.git",
    "directory": "js/packages/eyes-selenium"
  },
  "license": "SEE LICENSE IN LICENSE",
  "author": {
    "name": "Applitools Team",
    "email": "team@applitools.com"
  },
  "aliases": [
    "selenium"
  ],
  "main": "./dist/index.js",
  "types": "./types/index.d.ts",
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "lint": "node ../../node_modules/.bin/eslint '**/*.ts'",
    "build": "node ../../node_modules/.bin/tspc --project ./tsconfig.build.json",
    "generate:tests": "coverage-tests generate https://raw.githubusercontent.com/applitools/sdk.coverage.tests/universal-sdk/js/config.js --name 'eyes-selenium'",
    "test": "npm run test:coverage",
    "test:coverage": "npm run generate:tests && APPLITOOLS_BATCH_NAME='JS Coverage Tests: eyes-selenium' APPLITOOLS_BATCH_ID=$(uuidgen) MOCHA_GROUP=coverage node ../../node_modules/.bin/mocha './test/generic/*.spec.js' -r @applitools/test-utils/mocha-hooks/docker --parallel --jobs ${MOCHA_JOBS:-15} --exit",
    "setup": "npm run docker:setup",
    "docker:setup": "node ../../scripts/generate-docker-compose-config.js && docker-compose up -d",
    "docker:teardown": "docker-compose down",
    "upgrade:framework": "if [ ! -z $APPLITOOLS_FRAMEWORK_VERSION ]; then npm i --no-save selenium-webdriver@$APPLITOOLS_FRAMEWORK_VERSION; fi",
    "report": "bongo report --name 'js_selenium_4'",
    "postpublish": "bongo send-release-notification --name 'js_selenium_4'"
  },
  "dependencies": {
    "@applitools/eyes": "1.2.14",
    "@applitools/spec-driver-selenium": "1.5.47"
  },
  "devDependencies": {
    "@applitools/api-extractor": "1.2.21",
    "@applitools/bongo": "^4.0.0",
    "@applitools/sdk-coverage-tests": "3.0.2",
    "@applitools/test-utils": "1.5.17",
    "@types/node": "^12.20.55",
    "@types/selenium-webdriver": "^4.1.1",
    "chromedriver": "112.0.0",
    "selenium-webdriver": "4.4.0"
  },
  "peerDependencies": {
    "selenium-webdriver": ">=3.6.0"
  },
  "engines": {
    "node": ">=12.13.0"
  }
}
