{
  "name": "@applitools/eyes-playwright",
  "version": "1.17.1",
  "description": "Applitools Eyes SDK for Playwright",
  "keywords": [
    "eyes-playwright",
    "applitools",
    "eyes",
    "test automation",
    "visual regression",
    "automation",
    "testing",
    "tests",
    "playwright"
  ],
  "homepage": "https://applitools.com",
  "bugs": {
    "url": "https://github.com/applitools/eyes.sdk.javascript1/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:applitools/eyes.sdk.javascript1.git",
    "directory": "js/packages/eyes-playwright"
  },
  "license": "SEE LICENSE IN LICENSE",
  "author": {
    "name": "Applitools Team",
    "email": "team@applitools.com"
  },
  "aliases": [
    "playwright"
  ],
  "main": "./dist/index.js",
  "types": "./types/index.d.ts",
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "lint": "eslint '**/*.ts'",
    "build": "tspc --project ./tsconfig.build.json",
    "generate:tests": "coverage-tests generate https://raw.githubusercontent.com/applitools/sdk.coverage.tests/universal-sdk/js/config.js --name 'eyes-playwright'",
    "test": "npm run test:coverage",
    "test:coverage": "npm run generate:tests && APPLITOOLS_BATCH_NAME='JS Coverage Tests: eyes-playwright' APPLITOOLS_BATCH_ID=$(uuidgen) MOCHA_GROUP=coverage mocha './test/generic/*.spec.js' --parallel --jobs ${MOCHA_JOBS:-15} --exit",
    "upgrade:framework": "if [ ! -z $APPLITOOLS_FRAMEWORK_VERSION ]; then npm i --no-save playwright@$APPLITOOLS_FRAMEWORK_VERSION; fi",
    "report": "bongo report --name 'playwright'",
    "postpublish": "bongo send-release-notification --name 'playwright'"
  },
  "dependencies": {
    "@applitools/eyes": "1.2.14",
    "@applitools/spec-driver-playwright": "1.3.13"
  },
  "devDependencies": {
    "@applitools/api-extractor": "1.2.21",
    "@applitools/bongo": "^4.0.0",
    "@applitools/sdk-coverage-tests": "3.0.2",
    "@applitools/test-utils": "1.5.17",
    "@types/node": "^12.20.55",
    "playwright": "^1.34.3"
  },
  "peerDependencies": {
    "playwright": ">=1.0.0"
  },
  "engines": {
    "node": ">=12.13.0"
  }
}
