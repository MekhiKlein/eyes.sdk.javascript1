{
  "name": "root",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/applitools/eyes.sdk.javascript1"
  },
  "scripts": {
    "render": "node ./scripts/render.js",
    "lint:commit": "commitlint --extends @commitlint/config-conventional --edit",
    "lint:changed": "FILES=$(git --no-pager diff --name-only --relative --diff-filter=d -- '*.ts' '*.js') && eslint $FILES",
    "lint:staged": "FILES=$(git --no-pager diff --name-only --relative --diff-filter=d --cached -- '*.ts' '*.js') && eslint $FILES",
    "postinstall": "cd .. && husky install ./js/.husky"
  },
  "workspaces": [
    "packages/bongo",
    "packages/api-extractor",
    "packages/test-utils",
    "packages/test-server",
    "packages/utils",
    "packages/logger",
    "packages/socket",
    "packages/req",
    "packages/image",
    "packages/snippets",
    "packages/spec-driver-browser-extension",
    "packages/spec-driver-selenium",
    "packages/spec-driver-webdriverio",
    "packages/spec-driver-webdriver",
    "packages/spec-driver-puppeteer",
    "packages/spec-driver-playwright",
    "packages/driver",
    "packages/screenshoter",
    "packages/nml-client",
    "packages/tunnel-client",
    "packages/ufg-client",
    "packages/execution-grid-client",
    "packages/core-base",
    "packages/core",
    "packages/eyes-api",
    "packages/eyes-images",
    "packages/eyes-selenium",
    "packages/eyes-webdriverio-5",
    "packages/eyes-nightwatch",
    "packages/eyes-playwright",
    "packages/eyes-puppeteer"
  ],
  "devDependencies": {
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@types/mocha": "^10.0.1",
    "@types/node": "^12.20.55",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "cli-table3": "^0.6.3",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "mocha": "^10.2.0",
    "mocha-multi": "^1.1.7",
    "prettier": "^2.8.3",
    "ts-node": "^10.9.1",
    "ts-patch": "^3.0.0-rc3",
    "typescript": "^5.1.3",
    "yargs": "^17.6.2"
  },
  "packageManager": "yarn@3.6.0"
}
